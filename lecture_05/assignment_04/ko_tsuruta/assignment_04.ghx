import math as m

def generate_plane(crv, p, new_z_offset):
    pt = crv.PointAt(p)
    tan = crv.TangentAt(p)
    
    pt.Z += new_z_offset
    plane = rg.Plane(pt, tan, (-1)*rg.Vector3d.ZAxis)
    plane.Rotate(m.radians(90), plane.ZAxis)
    
    return plane


