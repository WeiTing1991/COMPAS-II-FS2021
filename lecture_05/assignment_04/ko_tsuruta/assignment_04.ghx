HERE = os.path.dirname(ghenv.Component.OnPingDocument().FilePath)

# create tool from mesh and frame
mesh = Mesh.from_stl(os.path.join(HERE, 'vacuum_gripper.stl'))
frame = Frame([z_offset, 0, 0], [0, 0, 1], [0, 1, 0])
tool = Tool(mesh, frame, name='vacuum_gripper')
tool_at_origin = MeshArtist(mesh).draw()

if robot and robot.client and robot.client.is_connected:
    scene = PlanningScene(robot)
    if attached:
        robot.attach_tool(tool)
        scene.add_attached_tool()
    else:
        if robot.attached_tool:
            scene.remove_attached_tool()
            scene.remove_collision_mesh('attached_tool_link_collision_0')
            robot.detach_tool()
