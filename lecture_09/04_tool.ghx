from compas.robots import RobotModel
from compas.robots import LocalPackageMeshLoader
from compas_fab.robots import Robot
from compas_fab.robots import RobotSemantics
from compas_ghpython.artists import RobotModelArtist

HERE = os.path.dirname(ghenv.Component.OnPingDocument().FilePath)

models_path = os.path.join(HERE, '..', 'docker', 'irb910sc-planner')

urdf_file = os.path.join(models_path, 'abb_irb910sc_support', 'urdf', 'irb910sc_3_065.urdf')
srdf_file = os.path.join(models_path, 'abb_irb910sc_3_065_moveit_config', 'config', 'irb910.srdf')

model = RobotModel.from_urdf_file(urdf_file)
semantics = RobotSemantics.from_srdf_file(srdf_file, model)
loader = LocalPackageMeshLoader(models_path, 'abb_irb910sc_support')

model.load_geometry(loader)

robot = Robot(model, semantics=semantics)
robot.artist = RobotModelArtist(model)
robot.info()</item>
