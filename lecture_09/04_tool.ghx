                    <item name="CodeInput" type_name="gh_string" type_code="10">import os
from compas.datastructures import Mesh
from compas.geometry import Frame
from compas_fab.robots import Tool
from compas_ghpython.artists import RobotModelArtist

HERE = os.path.dirname(ghenv.Component.OnPingDocument().FilePath)

if robot:
    if attach:
        vmesh = Mesh.from_stl(os.path.join(HERE, 'vacuum-gripper-visual.stl'))
        cmesh = Mesh.from_stl(os.path.join(HERE, 'vacuum-gripper-collision.stl'))
        frame = Frame([0, 0, -0.04328], [0, 0, 1], [0, 1, 0])
        tool = Tool(visual=vmesh, frame_in_tool0_frame=frame, collision=cmesh)
        robot.attach_tool(tool)
    if detach:
        robot.detach_tool()
