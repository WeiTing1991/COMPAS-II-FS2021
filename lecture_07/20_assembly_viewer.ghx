from compas_ghpython.artists import MeshArtist
#from helpers import traversal_buildup_sequence
#from helpers import traversal_breadth_first_ordering
from helpers import traversal_linearly_ordered

if assembly:
    # Load trajectory
    trajectory = assembly.element(i).trajectory
    if trajectory:
        trajectory = JointTrajectory(trajectory)
    
    ghenv.Component.Message = "Element {} loaded".format(i) if trajectory else 'Element {} has no trajectory'.format(i)
    
    elements = []
    for j, n in enumerate(traversal_linearly_ordered(assembly)):
